			"WikibaseQuality\\": "includes/",
			"WikibaseQuality\\Api\\": "api/",
			"WikibaseQuality\\Specials\\": "specials/",
			"WikibaseQuality\\Tests\\": "tests/phpunit/"
		},
		"classmap": [
			"WikibaseQualityHooks.php"
		]
	}
