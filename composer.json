{
    "name": "wikidata/wikidata",
    "description": "Wikidata build, including related components used for wikidata.org.",
    "license": "GPL-2.0+",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.wikimedia.org/git/mediawiki/extensions/Wikibase.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wmde/Wikidata.org.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wmde/WikimediaBadges.git"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "propertysuggester/property-suggester": "~2.0.1",
        "wikibase/wikibase": "dev-wmf/1.25wmf11",
        "wikibase/wikimedia-badges": "dev-master",
        "wikibase/Wikidata.org": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Wikidata\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": "Wikidata\\SettingsFileGenerator::generateDefaultSettings",
        "post-update-cmd": "Wikidata\\SettingsFileGenerator::generateDefaultSettings"
    },
    "config": {
        "autoloader-suffix": "wikidata_1_25wmf11"
    }
}